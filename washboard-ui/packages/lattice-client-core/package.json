{
  "name": "@wasmcloud/lattice-client-core",
  "packageManager": "yarn@4.0.2",
  "private": true,
  "version": "0.2.0",
  "description": "Framework-agnostic core package for the wasmCloud Lattice Client",
  "author": "wasmCloud",
  "repository": {
    "type": "git",
    "url": "https://github.com/wasmcloud/wasmcloud.git",
    "directory": "washboard-ui/packages/lattice-client-core"
  },
  "homepage": "https://tanstack.com/query",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./build/index.d.ts",
        "development": "./src/index.ts",
        "default": "./build/index.js"
      },
      "require": {
        "types": "./build/index.d.ts",
        "development": "./src/index.ts",
        "default": "./build/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "build",
    "src"
  ],
  "scripts": {
    "lint": "yarn lint:eslint",
    "lint:fix": "yarn lint:eslint:fix",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "test:types": "tsc",
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@types/eslint": "~8.56.10",
    "@wasmcloud/eslint-config": "workspace:^",
    "@wasmcloud/tsconfig": "workspace:^",
    "type-fest": "^4.18.2"
  },
  "dependencies": {
    "nats.ws": "^1.26.0",
    "tsup": "^8.0.2"
  }
}
