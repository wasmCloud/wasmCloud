{
  /* Visit https://aka.ms/tsconfig to read more about this file */
  "compilerOptions": {
    /* Language and Environment */
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,

    /* Source code */
    "paths": {
      "wasi:http/types@0.2.0": [ "./generated/types/wasi-http-types.d.ts" ],
      "wasmcloud:secrets/reveal@0.1.0-draft": [ "./generated/types/interfaces/wasmcloud-secrets-reveal.d.ts" ],
      "wasmcloud:secrets/store@0.1.0-draft": [ "./generated/types/interfaces/wasmcloud-secrets-store.d.ts" ],
    },
    "allowJs": false,

    /* Generation */
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true
  }
}
